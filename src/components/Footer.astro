---
import Main from "./logos/Main.astro";
---

<section class="">
  <hr class="border-[rgb(31,32,47)] self-center mx-auto mt-[100px] mb-[50px]" />

  <div class="flex items-center justify-center">
    <div class="flex flex-col items-center">
      <div class="mb-2">
        <a
          title="Twitter"
          href="https://twitter.com/treslog"
          target="_blank"
          rel="noopener noreferrer"
          class="ml-3 text-gray text-sm hover:text-white font-medium duration-300">Twitter</a
        >
        <a
          title="LinkedIn"
          href="https://linkedin.com/in/tri"
          target="_blank"
          rel="noopener noreferrer"
          class="ml-3 text-gray text-sm hover:text-white font-medium duration-300">LinkedIn</a
        >
        <a
          title="GitHub"
          href="https://github.com/treslog"
          target="_blank"
          rel="noopener noreferrer"
          class="ml-3 text-gray text-sm hover:text-white font-medium duration-300">GitHub</a
        >
      </div>
      <a
        title="Treslog"
        href="https://treslog.com/"
        class="flex items-center ml-4 sm:ml-0 mb-3"
      >
        <svg
          class="mr-3 h-6 mt-2 fill-gray"
          xmlns="http://www.w3.org/2000/svg"
          viewBox="0 0 122.88 101.57"
          width="30"
          height="30"
        >
          <path
            fill-rule="evenodd"
            d="M47.53,61.26l.15.15a8.91,8.91,0,0,1-.18,12.46s-.11.13-.16.16a8.91,8.91,0,0,1-12.42-.1c-9.66-8.86-23.59-20-32.1-29.25a8.92,8.92,0,0,1,.11-13.12c5.41-6.71,24.44-22.15,32-29A8.93,8.93,0,0,1,49.12,13a1.66,1.66,0,0,1-.44.56l-27,24.78c4.32,4.19,8.09,7.31,11.86,10.43a172.63,172.63,0,0,1,14,12.48Zm27.82,0-.15.15a8.91,8.91,0,0,0,.18,12.46s.11.13.16.16A8.91,8.91,0,0,0,88,73.93c9.66-8.86,23.59-20,32.1-29.25A8.92,8.92,0,0,0,120,31.56c-5.41-6.71-24.44-22.15-32-29A8.93,8.93,0,0,0,73.76,13a1.77,1.77,0,0,0,.43.56l27,24.78c-4.32,4.19-8.1,7.31-11.86,10.43a172.63,172.63,0,0,0-14,12.48Z"
          >
          </path>
        </svg>

        <span
          class="self-center text-base whitespace-nowrap text-gray font-bold"
          >Treslog</span
        >
      </a>
    </div>
  </div>
</section>
